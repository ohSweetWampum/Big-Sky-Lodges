 <div class="dashboard-body is-flex is-flex-wrap-wrap is-justify-content-center">
    
    <h2 class="dashboard">Dashboard</h2> 
    
    <div class="header-card custom-header-card">
        <h1>Welcome {{username}}</h1>
        <h4 id="booking-title">Recent Bookings</h4>
        <div class="row justify-content-center"> <div class="col-auto"></div>
        
        <div class="reservation-list">
            <div class="list1">
                <table class="table">
                    <thead>
                        <th>Reservation Id</th>
                        <th>Room type</th>
                        <th>Check-in Date</th>
                        <th>Check-out Date</th>
                        <th>Action</th>
                    </thead>
                    <tbody id="reservationDisplay">
                    {{#each users as |user|}} 
                        <tr>
                            <td>{{id}}</td>
                            <td>{{room.room_type}}</td>
                            <td>{{check_in_date}} </td>
                            <td>{{check_out_date}} </td>
                            <td><button class="delete-btn delete-reservation-btn" data-id="{{id}}">delete</button></td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
                
            </div>
        </div>
    </div>
    </div></div>
</div>


   



